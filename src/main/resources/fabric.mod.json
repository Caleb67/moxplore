{
	"schemaVersion": 1,
	"id": "moxplore",
	"version": "${version}",
	"name": "Moxplore",
	"description": "Expands on the game in ways that make sense.",
	"authors": [
		"Caleb Hayes"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/moxplore/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "io.github.caleb67.moxplore.Moxplore",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "io.github.caleb67.moxplore.MoxploreClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "io.github.caleb67.moxplore.MoxploreDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"moxplore.mixins.json",
		{
			"config": "moxplore.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.23"
	},
	"suggests": {
		"another-mod": "*"
	}
}